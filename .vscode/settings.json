{
    "files.associations": {
        "base.css": "tailwindcss",
        "*-tw.css": "tailwindcss",
        "*.mdx": "markdown"
    },
    "prettier.documentSelectors": ["**/*.astro"],
    "[astro]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": true
    },
    "tailwindCSS.emmetCompletions": true
}
